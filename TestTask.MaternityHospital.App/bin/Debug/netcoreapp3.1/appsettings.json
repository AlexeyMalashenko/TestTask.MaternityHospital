{
  "ConnectionStrings": {
  },
  "AppSettings": {
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": {
      "LocalFile": {
        "Name": "File",
        "Args": {
          "path": "..\\..\\..\\Logs\\new-solution.json",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    },
    "Enrich": [ "FromLogContext" ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": { "maximumDestructuringDepth": 4 }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": { "maximumStringLength": 100 }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": { "maximumCollectionCount": 10 }
      }
    ],
    "Properties": {
      "ApplicationName": "TestTask.MaternityHospital",
      "ApplicationType": "WebApiExt"
    }
  }
}
