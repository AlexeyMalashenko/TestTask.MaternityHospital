{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "WriteTo": {
      "Console": { "Name": "Console" },
      "Seq": {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341/"
        }
        /*
          command to run:
              docker run -d --restart unless-stopped --name seq -e "ACCEPT_EULA=Y" -v "e:/Server/seqLogging:/data" -p 15999:80 -p 5341:5341 datalust/seq:latest
          url to browse:
              http://localhost:15999/
        */
      }
    }
  }
}
